!
! DATASET LEVEL METADATA
!
START_META     =   DATASET_ID
   ENTRY       =   "CC_CG_AUX_ECLAT_GUMICS_ION_North_QL"
END_META       =   DATASET_ID
!
START_META     =   DATA_TYPE
   ENTRY       =   "CG>CAA_GRAPHIC"
END_META       =   DATA_TYPE
!
START_META     =   DATASET_TITLE
   ENTRY       =   "GUMICS-4 Ionospheric FACs, conductivites and Precipitation, Northern Hemisphere, 5 minute quicklook plots"
END_META       =   DATASET_TITLE
!
START_META     =   DATASET_DESCRIPTION
   ENTRY       =   "This dataset contains quicklook plots of Ionospheric conductivies, Field Aligned Currents (FACs) and particle precipitation from "
   ENTRY       =   "GUMICS-4(Janhunen et al., 1996, 2012) global MHD code's ionosphere in the Northern Hemisphere. A global snapshot of the Northern "
   ENTRY       =   "hemisphere is presented at 5 minute intervals."
   ENTRY       =   ""
   ENTRY       =   "The data are provided in a magnetic local time/magnetic latitude coordinate system with noon at the top and dusk to the left, at 1 hour "
   ENTRY       =   "cadence. The colour scale for each subplot is provided on its right side."
   ENTRY       =   ""
   ENTRY       =   "Top left panel shows the height integrated Pedersen conductivity and the top right panel shows the height integrated Hall conductivity."
   ENTRY       =   "Bottom left panel presents FACs in the ionosphere and the bottom right panel presents particle precipitation into the ionosphere from the "
   ENTRY       =   "magnetosphere."
   ENTRY       =   ""
   ENTRY       =   "The Cyan coloured contours are ionospheric equi-potential contours from GUMICS-4 Ionosphere and the dotted circle is the open-closed "
   ENTRY       =   "field line boundary in GUMICS-4."
   ENTRY       =   ""
END_META       =   DATASET_DESCRIPTION
!
START_META     =   CONTACT_COORDINATES
   ENTRY       =   "Minna Palmroth>GUMICS-4 Simulation contact>minna.palmroth@fmi.fi"
   ENTRY       =   "Gabor Facsko>facsko.gabor@csfk.mta.hu"
   ENTRY       =   "Chandrasekhar Reddy Anekallu>chandrasekhar.anekallu@fmi.fi"
END_META       =   CONTACT_COORDINATES
