    PARAMETER_TYPE      = "Data"
    ENTITY              = "Observatory"
    PROPERTY            = "Magnitude"
    FLUCTUATIONS        = "Waveform"
    CATDESC             = "GSE Latitude of spacecraft 3 axis, angle from Ecliptic Plane"
    UNITS               = "deg"
    SI_CONVERSION       = "1.0>degree"
    SIZES               = 1
    VALUE_TYPE          = FLOAT
    SIGNIFICANT_DIGITS  = 6
    FILLVAL             = -1.0e31
    QUALITY             = 2
    FIELDNAM            = "GSE Latitude of spacecraft 3 axis"
    LABLAXIS            = "C3 Lat (GSE)"
    DEPEND_0            = time_tags__C3_CP_AUX_SPIN_AXIS
END_VARIABLE   = sc_lat__C3_CP_AUX_SPIN_AXIS

