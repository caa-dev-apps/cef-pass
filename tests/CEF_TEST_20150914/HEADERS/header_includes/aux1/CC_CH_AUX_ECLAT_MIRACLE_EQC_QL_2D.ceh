!
! DATASET LEVEL METADATA
!
START_META     =   DATASET_ID
   ENTRY       =   "CC_CG_AUX_ECLAT_MIRACLE_EQC_QL_2D"
END_META       =   DATASET_ID
!
START_META     =   DATA_TYPE
   ENTRY       =   "CG>CAA_GRAPHIC"
END_META       =   DATA_TYPE
!
START_META     =   DATASET_TITLE
   ENTRY       =   "MIRACLE Equivalent Current Vectors, Northern Fennoscandia, six quicklook plots with 1 min time resolution."
END_META       =   DATASET_TITLE
!
START_META     =   DATASET_DESCRIPTION
   ENTRY       =   "This dataset contains quicklook plots of the ionospheric equivalent currents representing the divergence free"
   ENTRY       =   " part of total ionospheric currents. Equivalent currents have been derived from the measurements of the "
   ENTRY       =   "MIRACLE magnetometer network (IMAGE) with the method of Spherical Elementary Current Systems (SECS)."
   ENTRY       =   " For more information about SECS, see the paper by Amm and Viljanen (Earth Planets Space, 51, 431-440,1999)." 
   ENTRY       =   " The quicklook plots show the equivalent current pattern on a map with the Fennoscandian coastlines."
   ENTRY       =   " "
END_META       =   DATASET_DESCRIPTION
!
START_META     =   CONTACT_COORDINATES
   ENTRY       =   "Kirsti Kauristie>MIRACLE data provider>kirsti.kauristie@fmi.fi"
END_META       =   CONTACT_COORDINATES
!
START_META     =   TIME_RESOLUTION
   VALUE_TYPE  =   FLOAT
   ENTRY       =   360.0
END_META       =   TIME_RESOLUTION
!
