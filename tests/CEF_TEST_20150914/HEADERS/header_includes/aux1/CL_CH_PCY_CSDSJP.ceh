    PARAMETER_TYPE      = "Data"

    ENTITY              = "Observatory"

    PROPERTY            = "Magnitude"

    FLUCTUATIONS        = "Waveform"

    CATDESC             = "JSOC (CSDS JP) Cluster, Sunspot number, 12 month smoothed"

    UNITS               = "unitless"

    SI_CONVERSION       = "1.0>(number)"

    SIZES               = 1

    VALUE_TYPE          = FLOAT

    SIGNIFICANT_DIGITS  = 3

    FILLVAL             = -1.0e31

    QUALITY             = 2

    FIELDNAM            = "JSOC (CSDS JP) Cluster, Sunspot number"

    LABLAXIS            = "Sunspot num"

    DEPEND_0            = time_tags__CL_JP_PCY

END_VARIABLE   = Sunspot_number__CL_JP_PCY



