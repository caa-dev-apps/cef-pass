    UNITS               = "s"

    SI_CONVERSION       = "1.0>s"

    SIZES               = 1

    VALUE_TYPE          = FLOAT

    SIGNIFICANT_DIGITS  = 6

    FILLVAL             = -1.0e28

    FIELDNAM            = "Half width of event interval"

    LABLAXIS            = "s"

END_VARIABLE    =  half_interval__CL_JP_PCY



