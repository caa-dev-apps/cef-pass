START_VARIABLE  =  time_tags__CL_JP_PCY

    PARAMETER_TYPE      = "Support_Data"

    CATDESC             = "Interval centred time tag"

    UNITS               = "s"

    SI_CONVERSION       = "1.0>s"

    SIZES               = 1

    VALUE_TYPE          = ISO_TIME

    SIGNIFICANT_DIGITS  = 24

    FILLVAL             = 9999-12-31T23:59:59Z

    FIELDNAM            = "Universal Time"

    LABLAXIS            = "UT"

    DELTA_PLUS          = half_interval__CL_JP_PCY

    DELTA_MINUS         = half_interval__CL_JP_PCY

END_VARIABLE    =  time_tags__CL_JP_PCY



START_VARIABLE  =  half_interval__CL_JP_PCY

    PARAMETER_TYPE      = "Support_Data"

