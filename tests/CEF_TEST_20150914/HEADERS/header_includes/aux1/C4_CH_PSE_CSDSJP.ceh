END_META       =   METADATA_VERSION



START_VARIABLE  =  time_tags__C4_JP_PSE

    PARAMETER_TYPE      = "Support_Data"

    CATDESC             = "Event time tag"

    UNITS               = "s"

    SI_CONVERSION       = "1.0>s"

    SIZES               = 1

    VALUE_TYPE          = ISO_TIME

    SIGNIFICANT_DIGITS  = 24

    FILLVAL             = 9999-12-31T23:59:59Z

    FIELDNAM            = "Universal Time"

    LABLAXIS            = "UT"

    DELTA_PLUS          = half_interval__C4_JP_PSE

    DELTA_MINUS         = half_interval__C4_JP_PSE

END_VARIABLE    =  time_tags__C4_JP_PSE



START_VARIABLE  =  half_interval__C4_JP_PSE

    PARAMETER_TYPE      = "Support_Data"

    CATDESC             = "Half event interval length"

    UNITS               = "s"

    SI_CONVERSION       = "1.0>s"

    SIZES               = 1

    VALUE_TYPE          = FLOAT

    SIGNIFICANT_DIGITS  = 6

    FILLVAL             = -1.0e28

    FIELDNAM            = "Half width of event interval"

    LABLAXIS            = "s"

END_VARIABLE    =  half_interval__C4_JP_PSE



START_VARIABLE = event_code__C4_JP_PSE

    PARAMETER_TYPE      = "Data"

    ENTITY              = "Observatory"

    PROPERTY            = "Status"

    FLUCTUATIONS        = "Waveform"

    CATDESC             = "JSOC (CSDS JP) Cluster, Code for predicted event"

    UNITS               = "unitless"

    SI_CONVERSION       = "1.0>(code)"

    SIZES               = 1

    VALUE_TYPE          = CHAR

    SIGNIFICANT_DIGITS  = 3

    FILLVAL             = "---"

    QUALITY             = 2

    FIELDNAM            = "JSOC (CSDS JP) Cluster, Code for predicted event"

    LABLAXIS            = "Event Code"

    DEPEND_0            = time_tags__C4_JP_PSE

END_VARIABLE   = event_code__C4_JP_PSE



