START_VARIABLE = sc_r1_xyz_gse__C4_JP_PSE

    PARAMETER_TYPE      = "Data"

    ENTITY              = "Observatory"

    PROPERTY            = "Vector"

    FLUCTUATIONS        = "Waveform"

    CATDESC             = "JSOC (CSDS JP) Cluster, Spacecraft position at predicted event"

    UNITS               = "km"

    SI_CONVERSION       = "1.0e3>m"

    TENSOR_ORDER        = "1"

    COORDINATE_SYSTEM   = "GSE>Geocentric Solar Ecliptic"

    REPRESENTATION_1    = "x", "y", "z"

    SIZES               = 3

    VALUE_TYPE          = FLOAT

    SIGNIFICANT_DIGITS  = 6

    FILLVAL             = -1.0e31

    QUALITY             = 2

    FIELDNAM            = "JSOC (CSDS JP) Cluster, Position of sc at predicted event"

    LABLAXIS            = "Position of sc"

    DEPEND_0            = time_tags__C4_JP_PSE

    LABEL_1             = "x (GSE) pred", "y (GSE) pred", "z (GSE) pred"

END_VARIABLE   = sc_r1_xyz_gse__C4_JP_PSE



